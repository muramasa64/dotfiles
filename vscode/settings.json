{
    "workbench.startupEditor": "newUntitledFile",
    "workbench.fontAliasing": "default",
    "breadcrumbs.enabled": true,
    "editor.fontFamily": "Ricty-Regular",
    "editor.fontSize": 14,
    "editor.lineHeight": 0,
    "editor.letterSpacing": 0,
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Solarized Dark",
    "explorer.confirmDelete": false,
    "editor.fontLigatures": true,
    "markdownlint.run": "onSave",
    "markdownlint.config": {
        "no-inline-html": false,
        "ul-indent": {
            "indent": 4
        },
        "no-bare-urls": false,
        "MD028": false,
        "MD024": false
    },
    "editor.formatOnSave": true,
    "[elm]": {
        "editor.formatOnSave": true
    },
    "elm.analyseEnabled": true,
    "elm.analyseCommand": "/Users/isobe/.nodebrew/current/bin/elm-analyse",
    "elm.compiler": "/Users/isobe/.nodebrew/current/bin/elm",
    "elm.makeCommand": "/Users/isobe/.nodebrew/current/bin/elm",
    "elm.formatCommand": "/Users/isobe/.nodebrew/current/bin/elm-formatelm-format",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "files.exclude": {
        "**/.*.un~": true
    },
    "editor.renderWhitespace": "boundary",
    "editor.tabCompletion": "on",
    "editor.wordWrap": "on",
    "aws.samcli.location": "/usr/local/bin/sam",
    "aws.telemetry": true,
    "terminal.external.osxExec": "iTerm.app",
    "markdown.preview.scrollEditorWithPreview": false,
    "markdown.preview.scrollPreviewWithEditor": false,
    "[markdown]": {
        "editor.tabSize": 4,
        "editor.insertSpace": true
    },
    "japanese-proofreading.textlint.外来語カタカナ表記": false,
    "markdown.preview.scrollPreviewWithEditor": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features",
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "C_Cpp.formatting": "Disabled",
    "markdownlint.customRules": [
        "{docsmsft.docs-linting}/markdownlint-custom-rules/rules.js"
    ],
    "yaml.schemas": {
        "file:///toc.schema.json": "/toc\\.yml/i"
    },
    "cSpell.userWords": [
        "adcs",
        "authn",
        "intune",
        "intunewin",
        "jamf",
        "kusto",
        "mdatp",
        "ndes",
        "netskope",
        "passwordless",
        "sspr",
        "totp",
        "web",
        "web authn"
    ],
    "explorer.confirmDragAndDrop": false,
    "japanese-proofreading.textlint.ひらく漢字": false
}
